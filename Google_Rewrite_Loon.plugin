[Rewrite]
https?:\/\/(www\.)?g\.cn 302 https://www.google.com
^https?:\/\/(www\.)?google\.cn 302 https://www.google.com
^https?:\/\/(www\.)?google\.cn\/search 302 https://www.google.com/search

[MITM]
hostname = www.google.cn, www.g.cn